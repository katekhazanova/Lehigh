/* Body */
 @media print {
	a[href]:after { content: none !important; }
	
	.hidden-print,
	  tr.hidden-print,
	  th.hidden-print,
	  td.hidden-print {
	    display: none !important;
	  }
}

body {
	color: #000000;
	font-size: 12px;
}


/* Text */

h1 { font-size: 22px;margin-top: 5px; }
h2 {font-size: 20px;}
h3 {font-size: 18px;}
h4 {font-size: 16px;}

hr {
	border-style: none; 
	border-top: solid #55ccdd 1px;
}

.subtle { 
	color: #888888;
	font-weight: normal;
}

.locked { 
	background-color: #999999;
}

.redText { 
	color: #6f3d05; 
}

.navbar-default .navbar-text {
	color: #333333;
}

.badge-notifications {
	display: inline-block;
